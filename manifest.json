{
  "name": "Homes.mil Result Extractor",
  "description" : "A script to extract the home.mil results for easier manipulation",
  "version": "0.2",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "js": [
      "contentscript.js",
      "jquery-3.6.0.min.js"
    ],
    "matches": [
      "*://homes.mil/*",
      "*://www.homes.mil/*"
    ]
  }],
  "web_accessible_resources": [{
    "resources": [
      "tool.html",
      "script.js",
      "adrum-disable.js"
    ],
    "matches": ["<all_urls>"],
    "extension_ids": []
  }],
  "permissions": [
    "tabs",
    "storage",
    "webNavigation"
  ]
}