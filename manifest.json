{
  "name": "Homes.mil Result Extractor",
  "description" : "A script to extract the home.mil results for easier manipulation",
  "version": "0.1",
  "manifest_version": 2,
  "content_scripts": [{
    "js": [
      "contentscript.js",
      "jquery-3.6.0.min.js"
    ],
    "matches": [
      "*://homes.mil/*",
      "*://www.homes.mil/*"
    ]
  }],
  "web_accessible_resources": [
    "tool.html",
    "script.js"
  ],

  "permissions": [
    "tabs",
    "storage",
    "webNavigation"
  ]
}